// https://leetcode.com/problems/remove-duplicates-from-sorted-array/

#include <bits/stdc++.h>
using namespace std;

class Solution
{
public:
    int removeDuplicates(vector<int> &nums)
    {
        int i = 0, j = 0;
        int n = nums.size();
        
        while (j < n)
        {
            int current = nums[j];
            while (j < n && nums[j] == current)
            {
                ++j;
            }
            
        }
        
    }
};